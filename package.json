{
  "name": "wm-core",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && npm run copy:emails",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "migrate": "prisma migrate deploy",
    "migration:create": "prisma migrate dev --name",
    "start": "nest start",
    "start:dev": " npm run build && npm run copy:emails && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json",
    "pretest:e2e": "docker-compose up -d && dotenv -e .e2e.env -- prisma generate && dotenv -e .e2e.env -- prisma migrate reset -f",
    "posttest:e2e": "docker-compose stop && docker-compose rm -f test-db && docker-compose rm -f test-redis",
    "copy:emails": "cpx 'src/mail/templates/**' 'dist/src/mail/templates/'"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs/bull": "^0.3.1",
    "@nestjs/common": "^8.0.6",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^8.0.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/swagger": "^5.0.9",
    "@prisma/client": "^2.29.1",
    "bcrypt": "^5.0.1",
    "bull": "^3.20.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "handlebars": "^4.7.7",
    "ngeohash": "^0.6.3",
    "nodemailer": "^6.5.0",
    "nodemailer-mock-transport": "^1.3.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.1.0",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^8.0.6",
    "@types/bcrypt": "^3.0.0",
    "@types/bull": "^3.15.0",
    "@types/express": "^4.17.8",
    "@types/faker": "^5.1.7",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/nodemailer": "^6.4.0",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "cpx": "^1.5.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^5.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "prisma": "^2.29.1",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
